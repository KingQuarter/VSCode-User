    //== =======================================================================
    //== PowerShell
    //== =======================================================================

    "terminal.integrated.profiles.windows": {
        "PowerShell": {
            "source": "PowerShell",
            "icon": "terminal-powershell",
            "args": ["-ExecutionPolicy", "Bypass"]
        }
    },
    "terminal.integrated.defaultProfile.windows": "PowerShell",

    //= ========================================================================
    //= Extensions
    //= ========================================================================

    //== =======================================================================
    //== Better Comments
    //== =======================================================================

    "better-comments.tags": [
        {
            "tag": "! ",
            "color": "#ff6188",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "? ",
            "color": "#ffd866",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "note: ",
            "color": "#ffd866",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "todo: ",
            "color": "#fc9867",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "// ",
            "color": "#727072",
            "strikethrough": true,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "~ ",
            "color": "#727072",
            "strikethrough": true,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "* ",
            "color": "#a9dc76",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "= ",
            "color": "#86b7fc",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "== ",
            "color": "#ab9df2",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "=== ",
            "color": "#78dce8",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
    ],

    //== =======================================================================
    //== Comment Box
    //== =======================================================================

    "commentBox.styles": {
        "defaultStyle": {
            "capitalize": false,
            "textAlignment": "center",
            "boxWidth": 80,

            "commentStartToken": "/*= ",
            "topEdgeToken": "=",
            "topRightToken": " =*/",

            "leftEdgeToken": "/*= ",
            "rightEdgeToken": " =*/",

            "bottomLeftToken": "/*= ",
            "bottomEdgeToken": "=",
            "commentEndToken": " =*/",
        }
    },
    
