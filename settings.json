{
    //= ========================================================================
    //= Built-in
    //= ========================================================================

    "security.workspace.trust.untrustedFiles": "open",
    "files.exclude": {
        "**/.git": false,
        "**/.git**": true,
        "**/*.bak": true,
        "**/*.exe": true,
        "**/*.jpg": true,
        "**/*.png": true
    },
    "launch": {
        "configurations": [],
        "compounds": []
    },

    "[git-commit]": {
        "editor.rulers": [50, 72]
    },

    //== =======================================================================
    //== Color Customization
    //== =======================================================================

    //=== ======================================================================
    //=== Editor Token
    //=== ======================================================================

    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": [
                    "constant.language.ahk2",
                    "keyword.control.ahk2",
                    "keyword.control.directive.ahk2",
                    "storage.modifier.ahk2",
                    "storage.type.class.ahk2",
                    "storage.type.function.ahk2",
                    "variable.language.ahk2",
                    "variable.language.this.ahk2",
                    "variable.language.thishotkey.ahk2",
                ],
                "settings": {
                    "fontStyle": "italic"
                }
            },
            {
                "scope": [
                    "entity.name.type.class",
                    "keyword.keys.ahk2",
                    "keyword.operator.ahk2",
                    "keyword.operator.expression.ahk2",
                    "markup.underline",
                    "support.function.ahk2",
                ],
                "settings": {
                    "fontStyle": ""
                }
            },
        ],
        "[Monokai Pro*]":
        {
            "textMateRules": [
                {
                    "scope": [
                        "storage.type.class.ahk2"
                    ],
                    "settings": {
                        "foreground": "#FF6188"
                    }
                },
            ]
        }
    },
    "editor.semanticTokenColorCustomizations": {
        "enabled": true,
    },

